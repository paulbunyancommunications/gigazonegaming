{
  "name": "paulbunyannet/gigazone-gaming",
  "description": "Gigazone Gaming website",
  "authors": [
    {
      "name": "Nelson Castillo",
      "email": "castillo@paulbunyan.net"
    },
    {
      "name": "Nate Nolting",
      "email": "naten@paulbunyan.net"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "preferred-install": "dist",
    "discard-changes": true,
    "php": ">=7.1.*"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "4.7.6",
        "dist": {
          "type": "zip",
          "url": "https://wordpress.org/wordpress-4.7.6.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.0.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "pmenard/media-tags",
        "type": "wordpress-plugin",
        "version": "3.2.0.2",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/media-tags.3.2.0.2.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "nacin/link-manager",
        "type": "wordpress-plugin",
        "version": "0.1-beta",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/link-manager.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "archetyped/cornerstone",
        "type": "wordpress-plugin",
        "version": "0.7.5",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/cornerstone.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "lopo/duplicate-post",
        "type": "wordpress-plugin",
        "version": "3.0.3",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/duplicate-post.3.0.3.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mungobbq/enable-media-replace",
        "type": "wordpress-plugin",
        "version": "3.0.5",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/enable-media-replace.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "figureone/insert-pages",
        "type": "wordpress-plugin",
        "version": "3.1.6",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/insert-pages.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wp/rest-api",
        "type": "wordpress-plugin",
        "version": "2.0",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/rest-api.2.0-beta15.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "disqus/disqus-comment-system",
        "type": "wordpress-plugin",
        "version": "2.86",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/disqus-comment-system.2.86.zip"
        },
        "require": {
          "composer/installers": "v1.0.7"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "paulbunyancommunications/mailtrapper",
        "type": "git",
        "version": "master",
        "source": {
          "url": "https://github.com/paulbunyancommunications/mailtrapper.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.2.*",
    "cartalyst/sentinel": "2.0.*",
    "doctrine/dbal": "~2.5",
    "wordpress/wordpress": "4.7.6",
    "fancyguy/webroot-installer": "1.0.0",
    "symfony/yaml": "~2.4",
    "paulbunyannet/autoversion": "~1.0",
    "pmenard/media-tags": "3.2.0.2",
    "nacin/link-manager": "0.1-beta",
    "lopo/duplicate-post": "*",
    "paulbunyannet/bandolier": "~1.5",
    "twig/twig": "^1.24",
    "cocur/slugify": "^1.4",
    "archetyped/cornerstone": "*",
    "anahkiasen/underscore-php": "^2.0",
    "mungobbq/enable-media-replace": "*",
    "figureone/insert-pages": "*",
    "tedivm/stash": "^0.14.1",
    "wp/rest-api": "*",
    "vlucas/phpdotenv": "^2.2",
    "rcrowe/twigbridge": "0.9.3",
    "paulbunyannet/premailer": "^1.0",
    "paulbunyannet/wordpress-artisan": "^1.0",
    "sunra/php-simple-html-dom-parser": "v1.5.2",
    "league/csv": "^8.1",
    "laravelcollective/html": "5.2.*",
    "constantcontact/constantcontact": "2.1.*",
    "spatie/laravel-backup": "^3.7",
    "rollbar/rollbar": "~0.18.0",
    "jenssegers/rollbar": "^1.5",
    "timber/timber": "dev-master#512e3cf0526dfc9f15bfffa56ea4804bb36e0192",
    "ircmaxell/password-compat": "^1.0",
    "greggilbert/recaptcha": "^2.1",
    "paulbunyannet/form_mail": "1.4.*",
    "propaganistas/laravel-phone": "^2.8",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "brandonwamboldt/utilphp": "1.1.*",
    "league/commonmark": "^0.15.0",
    "disqus/disqus-comment-system": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "5.4.*",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*",
    "barryvdh/laravel-ide-helper": "2.4.*",
    "phing/phing": "2.*",
    "codeception/codeception": "2.3.1",
    "codeception/c3": "2.0.12",
    "etsy/phan": "^0.9.1",
    "paulbunyancommunications/mailtrapper": "dev-master"
  },
  "extra": {
    "webroot-dir": "public_html/wp",
    "webroot-package": "wordpress/wordpress",
    "installer-paths": {
      "public_html/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "public_html/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "src/Wordpress/functions.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Wordpress\\": "src/Wordpress/",
      "Twig\\": "src/Twig/",
      "GigaZone\\": "src/GigaZone/",
      "GameDisplay\\": "src/GameDisplay/",
      "Pbc\\GigaZoneGaming\\": "src/paulbunyannet/gigazonegaming/src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "files": [
      "tests/acceptance/BaseAcceptance.php",
      "tests/functional/BaseFunctional.php",
      "tests/integration/BaseIntegration.php"
    ],
    "psr-4": {
      "Codeception\\Module\\": "src/Codeception/Module"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\"; #"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan ide-helper:generate; #",
      "php artisan ide-helper:meta; #",
      "php artisan optimize; #",
      "bash ${PWD}/composer_post_fixes.sh; #"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize; #",
      "bash ${PWD}/composer_post_fixes.sh; #"
    ],
    "test": [
      "# Running Tests, usage `composer test -- TEST_ARGUMENTS`; #",
      "curl --silent -k https://gist.githubusercontent.com/castillo-n/cd4390a0ae0a2f6e4ca1b8c9472d10c1/raw/$(git ls-remote https://gist.github.com/castillo-n/cd4390a0ae0a2f6e4ca1b8c9472d10c1 | grep HEAD | awk '{ print $1}')/testing.sh > testing.sh; #",
      "chmod u+x testing.sh; #"
    ],
    "test-failed": [
      "@test -- -g failed; #"
    ],
    "docker-assets": [
      "# Getting Docker Assets; #",
      "curl --silent https://raw.githubusercontent.com/paulbunyannet/bash/$(git ls-remote https://github.com/paulbunyannet/bash.git | grep HEAD | awk '{ print $1}')/docker/get_docker_assets.sh > get_docker_assets.sh; #",
      "bash ${PWD}/get_docker_assets.sh; #"
    ],
    "post-docker": [
      "# Commands to run after ./dock.sh is done ; #",
      "@front-end-assets; #"
    ],
    "front-end-assets": [
      "# Process front end assets, usage `composer front-end-assets ENVIRONMENT`; #",
      "yarn; #",
      "yarn run gulp; #"
    ],
    "docker-rebuild": [
      "# Rebuild Docker containers #",
      "@docker_assets; #",
      "docker-compose down -v; #",
      "docker-compose pull; #",
      "docker-compose up -d; #"
    ],
    "env-check": [
      "# Environment variable check #",
      "curl --silent -k https://gist.githubusercontent.com/paulbunyannet/21c889d6c4c45a9ea802607410e3d002/raw/$(git ls-remote https://gist.github.com/paulbunyannet/21c889d6c4c45a9ea802607410e3d002 | grep HEAD | awk '{ print $1}')/env_check.php > env_check; #",
      "chmod u+x env_check; #",
      "./env_check; #"
    ],
    "rollbar-deploy": [
      "# Rollbar deployment. Usage: `composer rollbar-deploy -- TOKEN ENVIRONMENT`; #",
      "curl --silent -k https://gist.githubusercontent.com/paulbunyannet/95fecfd2065def37f88ffdfe4250d608/raw/$(git ls-remote https://gist.github.com/paulbunyannet/95fecfd2065def37f88ffdfe4250d608 | grep HEAD | awk '{ print $1}')/rollbar_deploy.sh > rollbar_deploy.sh; #",
      "chmod u+x rollbar_deploy.sh; #",
      "./rollbar_deploy.sh; #"
    ],
    "codecov-deploy": [
      "# Codecov deployment. Usage: `composer codecov-deploy -- LOCATION/OF/COVERAGE.xml TOKEN`; #",
      "curl --silent -k https://gist.githubusercontent.com/paulbunyannet/b022b8fdb590ccb061222183e55193aa/raw/$(git ls-remote https://gist.github.com/paulbunyannet/b022b8fdb590ccb061222183e55193aa | grep HEAD | awk '{ print $1}')/codecov_deploy.sh > codecov_deploy.sh; #",
      "chmod u+x codecov_deploy.sh; #",
      "./codecov_deploy.sh; #"
    ]
  }
}
